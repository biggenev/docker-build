pipeline {
    agent any
	options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
		timestamps()
		}
    stages {
        stage('Create build image') {
            steps {
                echo 'Start'
				dir ('.'){
				sh 'docker build -t mysite:v9 . '
				}
				}
            }
        }
    }

